
{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    "type": "small",
  },
  "modules": [
    "title",
    "separator",
    "host",
    "os",
    {
        "type": "cpu",
        "key": "CPU",
        "format": "{1}"
    },
    {
        "type": "gpu",
        "key": "GPU",
        "format": "{2} {7} {9}"
    },
    {
        "type": "memory",
        "key": "RAM",
        "format": "{2}"
    }
  ]
}
